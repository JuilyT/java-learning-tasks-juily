package org.multithreading.part2;

import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.HashSet;
import java.util.Set;

public class CommonFileReader {
	public static void main(String[] args) {
		Set<String> lines = new HashSet<>();
		try {
		
		for (String string : lines) {
			System.out.println(string);
		}
		WikiReader wiki = new WikiReader();
		wiki.generateWikiFiles(lines);
	}
	
	public List<String> readFile() {
		BufferedReader reader = new BufferedReader(new FileReader("/home/juilykumari/Downloads/Multithreading_Task_2_java Keywords.txt"));
		String line;
		reader.readLine();
		while ((line = reader.readLine()) != null) {
			String[] values = line.split(",");
		      for (String str : values) {
		        if(str.length()>0)
		        {
		        	System.out.println(str);
		        	lines.add(str.trim());
		        }
		      }
			//lines.add(line);
		}
		reader.close();
	} catch (IOException e) {
	    e.printStackTrace();
	}
	}
}
